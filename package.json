{
  "name": "Frodo",
  "version": "1.0.0",
  "license": "GPL-3.0",
  "author": "Alphabean, Inc.",
  "scripts": {
    "serve": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve",
    "build:all": "yarn check-i18n --ci && yarn jest --ci && yarn build:pawket && yarn build:mixch",
    "build:pawket": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build",
    "build-ext": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=vue.config.ext.js vue-cli-service build",
    "build:mixch": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=vue.config.mixch.js vue-cli-service build",
    "build:server": "ncc build server/server.ts -o dist/server && cp node_modules/clvm/browser/blsjs.wasm dist/server",
    "lint": "vue-cli-service lint",
    "serve-ext": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=vue.config.ext.js vue-cli-service --env.NODE_ENV=development build-watch --mode development",
    "serve:mixch": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=vue.config.mixch.js vue-cli-service serve --port 8088",
    "check-i18n": "vue-i18n-extract report --vueFiles \"./src/**/*.?(js|ts|vue)\" --languageFiles \"./src/i18n/*.?(json|yml|yaml|js)\"",
    "test": "jest --coverage=false"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "@noble/bls12-381": "^1.4.0",
    "@scure/base": "^1.0.0",
    "@types/elliptic": "^6.4.14",
    "@types/sanitize-html": "^2.6.2",
    "@types/sortablejs": "^1.10.7",
    "@types/uuid": "^8.3.4",
    "bip39": "^3.0.4",
    "buefy": "^0.9.14",
    "buffer": "^6.0.3",
    "clvm_tools": "^0.9.5",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "elliptic": "^6.5.4",
    "fetch-intercept": "^2.4.0",
    "js-big-decimal": "^1.3.4",
    "mermaid": "^9.1.5",
    "monaco-editor": "^0.30.1",
    "monaco-editor-webpack-plugin": "^6.0.0",
    "pbkdf2-hmac": "^1.0.4",
    "qrcode.vue": "^1.0.0",
    "register-service-worker": "^1.7.1",
    "sanitize-html": "^2.7.0",
    "sortablejs": "^1.14.0",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
